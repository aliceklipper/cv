[ignore]
<PROJECT_ROOT>/node_modules/babel-plugin-flow-runtime/.*
<PROJECT_ROOT>/node_modules/styled-components/.*
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/temp/.*
<PROJECT_ROOT>/var/.*
<PROJECT_ROOT>/coverage/.*
<PROJECT_ROOT>/cache/.*

[options]
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^api/\(.*\)$' -> '<PROJECT_ROOT>/src/api/\1'
module.name_mapper='^components/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^config/\(.*\)$' -> '<PROJECT_ROOT>/src/config/\1'
module.name_mapper='^containers/\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^lib/\(.*\)$' -> '<PROJECT_ROOT>/src/lib/\1'
module.name_mapper='^messages/\(.*\)$' -> '<PROJECT_ROOT>/src/messages/\1'
module.name_mapper='^store/\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^types/\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'

module.name_mapper='^~$' -> '<PROJECT_ROOT>/src/'
module.name_mapper='^api$' -> '<PROJECT_ROOT>/src/api/'
module.name_mapper='^components$' -> '<PROJECT_ROOT>/src/components/'
module.name_mapper='^config$' -> '<PROJECT_ROOT>/src/config/'
module.name_mapper='^containers$' -> '<PROJECT_ROOT>/src/containers/'
module.name_mapper='^lib$' -> '<PROJECT_ROOT>/src/lib/'
module.name_mapper='^messages' -> '<PROJECT_ROOT>/src/messages/'
module.name_mapper='^store' -> '<PROJECT_ROOT>/src/store/'
module.name_mapper='^types$' -> '<PROJECT_ROOT>/src/types/'

module.name_mapper.extension='jpeg' -> '<PROJECT_ROOT>/image.jsx'
